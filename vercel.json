{
  "builds": [{ "src": "**/*.html", "use": "@vercel/static" }],
  "routes": [
    { "src": "/styles/(.*)", "dest": "/styles/$1" },
    { "src": "/js/(.*)", "dest": "/js/$1" },
    { "src": "/public/(.*)", "dest": "/public/$1" },
    { "src": "/pages/(.*)", "dest": "/pages/$1" },
    { "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|PNG))", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
